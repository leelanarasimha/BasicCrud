@page
@model BasicCrud.Pages.Users.ListModel
@{
    ViewData["Title"] = "List";
}

<h2>List</h2>

<h2>@Model.Message</h2>

<div>
    <table class="table">
        <thead>

        </thead>
        <tbody>
            @foreach (var user in Model.users)
            {
                <tr>
                    <td>@user.Name</td>
                    <td>@user.Email</td>
                    <td>@user.Age</td>
                </tr>
            }

        </tbody>
    </table>
</div>

